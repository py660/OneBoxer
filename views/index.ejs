  * {
    box-sizing: border-box;
    margin: 0;
    padding: 0;
  }

  html {
    scroll-behavior: smooth;
  }

  body {
    font-family: var(--font-family);
    background-color: var(--background-root);
    overflow-x: hidden;
  }

  .navbar {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 15px;
    background-color: var(--background-root);
    border-bottom: 1px solid var(--outline-dimmest);
  }

  .nav-alert {
    z-index: 0;
    display: inline-block;
    width: 100%;
    align-items: center;
    justify-content: space-between;
    margin-top: 10px;
    padding: 8px 15px;
    font-weight: 600;
    background-color: var(--accent-primary-dimmer);
    color: var(--foreground-default);
  }

  .nav-alert .nav-alert-delete {
    cursor: pointer;
    position: absolute;
    right: 15px;
  }

  .nav-alert .nav-alert-delete::after {
    content: "✕";
  }

  .navbar li {
    list-style-type: none;
  }

  .navbar .nav-logo {
    font-size: 18px;
  }

  .navbar .nav-logo svg.align-middle,
  .navbar .nav-logo img.align-middle {
    width: 25px;
    height: 25px;
    vertical-align: middle;
    margin-right: 6px;
  }

  .navbar .nav-logo:hover {
    color: var(--foreground-default);
  }

  .navbar a,
  .nav-links a {
    color: var(--foreground-default);
    font-size: 16px;
    font-weight: 600;
    transition: 0.3s;
  }

  .navbar a:hover,
  .nav-links a:hover {
    color: var(--accent-primary-stronger);
  }

  .navbar a.btn {
    font-size: 16px;
    padding: 5px 10px;
    border-radius: 10px;
  }

  .navbar a.btn:hover {
    color: var(--foreground-default);
  }

  .menu {
    display: flex;
    font-size: 16px;
    gap: 1.5em;
  }

  .menu li {
    padding: 5px 14px;
  }

  .nav-dropdown {
    position: relative;
    background-color: var(--background-root);
    border-radius: 8px;
    transition: 0.3s;
  }

  .nav-dropdown a:hover {
    color: var(--foreground-default);
  }

  .nav-dropdown .dropdown-link::after {
    content: "↓";
    margin-left: 5px;
  }

  .nav-dropdown:hover {
    background-color: var(--background-default);
    color: var(--foreground-default);
    border-radius: 8px;
  }

  .dropdown {
    z-index: 9999;
    background-color: var(--background-root);
    border: 1px solid var(--outline-dimmest);
    padding: 1em 0;
    position: absolute;
    display: none;
    top: 35px;
    border-radius: 8px;
  }

  .dropdown li + li {
    margin-top: 8px;
  }

  .dropdown li {
    text-align: center;
    width: 8em;
    padding: 7px;
    transition: 0.2s;
  }

  .dropdown li:hover {
    background-color: var(--accent-primary-dimmer);
  }

  .nav-dropdown:hover .dropdown {
    display: block;
  }

  .navbar input[type="checkbox"] {
    display: none;
  }

  .hamburger {
    display: none;
    font-size: 24px;
    font-weight: 800;
    padding: 2px 6px;
    border: 1.5px solid var(--background-root);
    border-radius: 8px;
    user-select: none;
    transition: 0.3s;
  }

  .hamburger:hover {
    background-color: var(--background-default);
    cursor: pointer;
  }

  .container {
    display: flex;
    flex-direction: column;
    gap: 64px;
    margin: 64px auto;
    max-width: 1000px;
  }

  @media (max-width: 1000px) {
    .container {
      display: flex;
      flex-direction: column;
      max-width: 600px;
      margin: 100px auto;
      margin-bottom: 0;
