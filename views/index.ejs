  input[type="text"],
  input[type="url"],
  input[type="number"],
  input[type="email"],
  input[type="password"],
  inpuy[type="time"],
  input[type="date"],
  input[type="datetime-local"],
  input[type="month"],
  input[type="week"],
  input[type="search"],
  input[type="tel"],
  textarea,
  select {
    font-family: var(--font-family-default);
    background: var(--background-default);
    color: var(--foreground-default);
    padding: 4px;
    font-size: 12px;
    border-radius: 12px;
    border: 1.5px var(--outline-dimmest) solid;
    outline: none;
    transition: 0.3s;
  }

  textarea {
    height: 150px;
  }

  input[type="text"]::placeholder,
  input[type="url"]::placeholder,
  input[type="number"]::placeholder,
  input[type="email"]::placeholder,
  input[type="password"]::placeholder,
  inpuy[type="time"]::placeholder,
  input[type="date"]::placeholder,
  input[type="datetime-local"]::placeholder,
  input[type="month"]::placeholder,
  input[type="week"]::placeholder,
  input[type="search"]::placeholder,
  input[type="tel"]::placeholder,
  textarea::placeholder,
  select::placeholder {
    color: var(--foreground-dimmest);
  }

  input[type="text"]:focus,
  input[type="url"]:focus,
  input[type="number"]:focus,
  input[type="email"]:focus,
  input[type="password"]:focus,
  inpuy[type="time"]:focus,
  input[type="date"]:focus,
  input[type="datetime-local"]:focus,
  input[type="month"]:focus,
  input[type="week"]:focus,
  input[type="search"]:focus,
  input[type="tel"]:focus,
  textarea:focus,
  select:focus {
    border-color: var(--accent-primary-dimmer);
  }

  input[type="text"]:disabled,
  input[type="url"]:disabled,
  input[type="number"]:disabled,
  input[type="email"]:disabled,
  input[type="password"]:disabled,
  inpuy[type="time"]:disabled,
  input[type="date"]:disabled,
  input[type="datetime-local"]:disabled,
  input[type="month"]:disabled,
  input[type="week"]:disabled,
  input[type="search"]:disabled,
  input[type="tel"]:disabled,
  select:disabled {
    opacity: 0.5;
  }

  input[type="file"] {
    display: none;
  }

  .file-container {
    position: relative;
  }

  .switch {
    position: relative;
    display: inline-block;
    width: 80px;
    height: 34px;
    margin-bottom: 20px;
  }

  .switch input[type="checkbox"] {
    opacity: 0;
    width: 0;
    height: 0;
  }

  .slider {
    background-color: var(--background-default);
    position: absolute;
    cursor: pointer;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    transition: 0.3s;
  }

  .slider:before {
    position: absolute;
    content: "";
    height: 23px;
    width: 23px;
    left: 7.5px;
    bottom: 5.5px;
    background-color: var(--foreground-default);
    transition: 0.3s;
  }

  input[type="checkbox"]:checked + .slider {
    background-color: var(--accent-primary-dimmer);
  }

  input[type="checkbox"]:checked + .slider:before {
    transform: translateX(42px);
  }

  .slider.round {
    border-radius: 34px;
  }

  .slider.round:before {
    border-radius: 50%;
  }

  .checkbox-container {
    display: block;
    position: relative;
    padding-left: 35px;
    margin-bottom: -40px;
    font-size: 18px;
    color: var(--foreground-dimmer);
  }

  .checkbox-container:last-child,
  .radio-container:last-child {
    margin-bottom: 0;
  }

  .checkbox-container input[type="checkbox"] {
    position: absolute;
    opacity: 0;
    cursor: pointer;
  }

  .checkbox-mark {
    position: absolute;
    top: 0;
    left: 0;
    height: 23px;
    width: 23px;
    background-color: transparent;
    border: 1.5px var(--outline-dimmest) solid;
    border-radius: 5px;
    cursor: pointer;
    transition: 0.3s;
  }

  .checkbox-container input[type="checkbox"]:checked ~ .checkbox-mark {
    background-color: var(--accent-primary-dimmer);
    border: 1.5px var(--accent-primary-dimmer) solid;
  }

  .checkbox-mark:after {
    content: "";
    position: absolute;
    display: none;
  }

  .checkbox-container input[type="checkbox"]:checked ~ .checkbox-mark:after {
    display: block;
  }

  .checkbox-container .checkbox-mark:after {
    left: 7px;
    top: 3px;
    width: 5px;
    height: 10px;
    border: 1.5px solid var(--foreground-default);
    border-width: 0 2px 2px 0;
    transform: rotate(45deg);
  }

  .checkbox-container input[type="checkbox"]:disabled ~ .checkbox-mark,
  .radio-container input[type="radio"]:disabled ~ .radio-mark {
    opacity: 0.4;
    cursor: auto;
  }

  .radio-container {
    display: block;
    position: relative;
    padding-left: 35px;
    margin-bottom: -40px;
    font-size: 18px;
    color: var(--foreground-dimmer);
  }

  .radio-container input[type="radio"] {
    position: absolute;
    opacity: 0;
  }

  .radio-mark {
    position: absolute;
    top: 0;
    left: 0;
    height: 23px;
    width: 23px;
    background-color: transparent;
    border: 1.5px var(--outline-dimmest) solid;
    border-radius: 50%;
    transition: 0.3s;
    cursor: pointer;
  }

  .radio-container input[type="radio"]:checked ~ .radio-mark {
    background-color: var(--accent-primary-dimmer);
    border: 1.5px var(--accent-primary-dimmer) solid;
  }

  .radio-mark:after {
    content: "";
    position: absolute;
    display: none;
  }

  .radio-container input[type="radio"]:checked ~ .radio-mark:after {
    display: block;
  }

  .radio-container .radio-mark:after {
    background-color: var(--foreground-default);
    top: 5.73px;
    left: 5.73px;
    width: 9px;
    height: 9px;
    border-radius: 50%;
  }

  table {
    width: 100%;
    border-collapse: collapse;
    border: 1.5px solid var(--outline-dimmest);
  }

  table td,
  table th {
    color: var(--foreground-dimmer);
    font-size: 17px;
    padding: 12px;
    border: 1.5px solid var(--outline-dimmest);
  }

  table th {
    background-color: var(--accent-primary-dimmer);
    color: var(--foreground-default);
    font-size: 22px;
    padding-top: 12px;
    padding-bottom: 12px;
    text-align: left;
  }

  table tr:nth-child(even) {
    background-color: var(--background-default);
  }

  .card-container {
    display: flex;
    flex-direction: row;
    justify-content: space-evenly;
    align-items: stretch;
    align-content: space-around;
    flex-wrap: wrap;
    gap: 25px;
  }

  .card {
    outline: 1.5px var(--outline-dimmest) solid;
    border-radius: 8px;
    flex: 1 0 47%;
    height: fit-content;
    transition: 0.3s;
  }

  .card:hover {
    outline-color: var(--outline-dimmer);
  }

  .card:active {
    outline-color: var(--accent-primary-dimmer);
  }

  .card img.card-cover,
  .card svg.card-cover {
    width: 100%;
    border-top-left-radius: 8px;
    border-top-right-radius: 8px;
    border-bottom-left-radius: 0px;
    border-bottom-right-radius: 0px;
  }

  .card h1,
  .card h2,
  .card h3,
  .card h4,
  .card h5,
  .card h6,
  .card p {
    padding: 15px;
  }

  .card > a,
  .card > strong,
  .card > b,
  .card > i,
  .card u,
  .card s {
    padding: 15px;
  }

  .badge {
    width: fit-content;
    font-size: 0.7em;
    padding: 2px 7px;
    margin-left: 8px;
    border-radius: 20px;
    font-weight: 600;
  }

  code {
    font-family: "IBM Plex Mono", sans-serif;
    font-size: 16px;
    background-color: var(--background-default);
    color: var(--foreground-dimmer);
    padding: 4px;
    border-radius: 8px;
  }

  .alert {
    width: 100%;
    font-size: 18px;
    font-weight: 600;
    padding: 11px 13px;
    line-height: 30px;
    border-radius: 11px;
    border: 1px solid transparent;
  }

  .alert-primary {
    background-color: var(--accent-primary-dimmest);
    color: var(--accent-primary-strongest);
    border: 1px solid var(--accent-primary-stronger);
  }

  .alert-positive {
    background-color: var(--accent-positive-dimmest);
    color: var(--accent-positive-strongest);
    border: 1px solid var(--accent-positive-stronger);
  }

  .alert-negative {
    background-color: var(--accent-negative-dimmest);
    color: var(--accent-negative-strongest);
    border: 1px solid var(--accent-negative-stronger);
  }

  .alert .alert-icon {
    width: 22px;
    height: 22px;
    vertical-align: middle;
    margin-right: 8px;
  }

  .footer {
    background-color: var(--background-default);
    color: var(--foreground-default);
    text-align: center;
    font-size: 19px;
    font-weight: 600;
    padding: 15px;
  }

  iframe {
    border-radius: 11px;
    border: 1px solid var(--outline-dimmest);
  }

  .spinner {
    width: 40px;
    height: 40px;
    border: 4px var(--background-root) solid;
    border-radius: 50%;
    border-top: 4px var(--accent-primary-default) solid;
    border-right: 4px var(--accent-primary-default) solid;
    border-bottom: 4px var(--accent-primary-default) solid;
    animation: spin 1.5s linear infinite;
  }

  .badge-teal {
    background-color: var(--accent-teal-default);
    color: var(--accent-teal-strongest);
  }

  .badge-red {
    background-color: var(--accent-red-dimmer);
    color: var(--accent-red-strongest);
  }

  .badge-orange {
    background-color: var(--accent-orange-default);
    color: var(--accent-orange-strongest);
  }

  .badge-blue {
    background-color: var(--accent-blue-dimmer);
    color: var(--accent-blue-strongest);
  }

  .badge-green {
    background-color: var(--accent-green-default);
    color: var(--accent-green-strongest);
  }

  .badge-purple {
    background-color: var(--accent-purple-dimmer);
    color: var(--accent-purple-strongest);
  }

  @keyframes spin {
    0% {
      transform: rotate(0deg);
    }

    100% {
      transform: rotate(360deg);
    }
  }

  @media (max-width: 800px) {
    .container {
      max-width: 80%;
    }

    .menu {
      display: none;
      position: absolute;
      background-color: var(--background-root);
      right: 0;
      left: 0;
      text-align: center;
      padding: 16px 0;
    }

    .menu li:hover {
      display: inline-block;
      transition: 0.3s ease;
    }

    .menu li + li {
      margin-top: 12px;
    }

    input[type="checkbox"]:checked ~ .menu {
      display: block;
    }

    .hamburger {
      display: block;
      color: var(--foreground-default);
    }

    .dropdown {
      top: 30px;
    }
  }

  @media (max-width: 600px) {
    h1 {
      font-size: 38px;
    }

    h2 {
      font-size: 32px;
    }

    h3 {
      /* font-size: 27px; */
    }

    h4 {
      font-size: 24px;
    }

    h5 {
      font-size: 21px;
    }

    h6,
    p {
      /* font-size: 18px; */
    }

    img {
      max-width: fit-content !important;
      width: 100%;
    }
  }

  ::-webkit-scrollbar {
    width: 10px;
  }

  ::-webkit-scrollbar:horizontal {
    height: 8px;
  }

  ::-webkit-scrollbar-track {
    background-color: var(--background-default);
  }

  ::-webkit-scrollbar-thumb {
    background-color: var(--background-higher);
  }

  ::selection {
    background-color: var(--accent-primary-default);
    color: var(--foreground-default);
  }
</style>
