  <meta property="og:site_name" content="<%= siteName %>" />
  <meta property="og:title" content="<%= title %>" />
  <meta property="og:type" content="image" />
  <meta property="og:url" content="https://oneboxer.repl.co" />
  <meta property="og:description" content="<%= description %>" />
</head>

<aside
  class="onebox allowlistedgeneric"
  data-onebox-src="https://oneboxer.repl.co"
>
  <header class="source">
    <a
      id="siteNameField"
      href="https://oneboxer.repl.co"
      target="_blank"
      rel="noopener nofollow ugc"
      ><%= siteName %></a
    >
  </header>

  <article class="onebox-body">
    <% if (image) { %>
    <!-- <div class="aspect-image" style="--aspect-ratio:690/345;"><img src="<%= image %>" class="thumbnail" width="690" height="345" data-dominant-color="F5F5F3" loading="lazy" style="aspect-ratio: 690 / 345;"></div> -->
    <% } %>

    <h3>
      <a
        id="titleField"
        href="https://oneboxer.repl.co"
        target="_blank"
        rel="noopener nofollow ugc"
        ><%= title %></a
      >
    </h3>

    <p id="descriptionField"><%= description %></p>
  </article>

  <div class="onebox-metadata"></div>

  <div style="clear: both"></div>
</aside>

<form autocomplete="off" method="get" action="/">
  <div class="inp">
    <label for="siteName">Site name (Author)</label>
    <input required type="text" id="siteName" name="siteName"
    placeholder="Oneboxer by Haroon" <% if (siteName !== "Oneboxer by Haroon") {
    %> value="<%= siteName %>" <% } %> >
  </div>
  <div class="inp">
    <label for="title">Title</label>
    <input required type="text" id="title" name="title" placeholder="Oneboxer"
    <% if (title !== "Oneboxer") { %> value="<%= title %>" <% } %> >
  </div>
  <div class="inp">
    <label for="description">Description</label>
    <textarea
      required
      id="description"
      name="description"
      placeholder="Create simple Discourse oneboxes (and Discord embeds) with this tool!"
    >
<% if (description !== "Create simple Discourse oneboxes (and Discord embeds) with this tool!") { %><%= description %><% } %></textarea
    >
  </div>
  <input class="btn btn-primary" type="submit" value="Generate" />
  <div class="nav-alert" id="nav-alert">
    <% if (siteName !== "Oneboxer by Haroon" && title !== "Oneboxer" && description !== "Create simple Discourse oneboxes (and Discord embeds) with this tool!") { %>
      https://oneboxer.repl.co<%= req.url %>
    <% } else { %>
    Your generated URL will display here...
    <% } %>
  </div>
</form>

<script>
  const siteName = document.querySelector("#siteNameField");
  const title = document.querySelector("#titleField");
  const description = document.querySelector("#descriptionField");

  const a = [siteName, title, description];

  document.querySelector("form").addEventListener("submit", (e) => {
    e.preventDefault();

    let url = new URL("/", "https://oneboxer.repl.co");

    try {
      Array.from(e.target.children).forEach((child, i) => {
        console.log(child.children[1]);
        a[i].innerText = child.children[1].value;

        url.searchParams.set(child.children[1].id, child.children[1].value);
      });
    } catch (e) {}

    document.querySelector("#nav-alert").innerText = url.toString();

    history.replaceState({}, "", url);
  });
</script>
